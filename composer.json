{
  "name": "webit/shortcut_statuscodes",
  "description": "Different HTTP statuscodes for shortcuts",
  "homepage": "https://github.com/webit-de/typo3-shortcut_statuscodes",
  "license": ["GPL-2.0+"],
  "type": "typo3-cms-extension",
  "require": {
    "php": ">=5.5.0",
    "typo3/cms-core": "^6.2 || ^7.6"
  },
  "autoload": {
    "psr-4": {
      "Webit\\ShortcutStatuscodes\\": "Classes/"
    }
  },
  "replace": {
    "shortcut_statuscodes": "self.version",
    "typo3-ter/shortcut_statuscodes": "self.version"
  }
}
